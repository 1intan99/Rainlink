{
  "name": "rainlink",
  "version": "0.7.2",
  "description": "Another lavalink wrapper but focus on stability and rich features",
  "repository": {
    "type": "git",
    "url": "https://github.com/RainyXeon/Rainlink.git"
  },
  "scripts": {
    "build": "npx tsc --build --verbose",
    "prettier": "npx prettier -w ./src",
    "build:doc": "typedoc --options ./typedoc.json --sort kind",
    "build:full": "npm run prettier && npm run build:eslint && npm run build && npm run build:doc",
    "build:eslint": "npx eslint ./src --fix"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "bot",
    "music",
    "lavalink.js",
    "lavalink-api",
    "discord.js",
    "eris.js",
    "oceanic.js",
    "lavalink",
    "wrapper",
    "lavalink-wrapper",
    "rainlink"
  ],
  "author": "RainyXeon",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^20.12.5",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "discord.js": "^14.14.1",
    "dokdo": "^1.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.4"
  }
}
