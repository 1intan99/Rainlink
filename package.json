{
  "name": "rainlink",
  "version": "0.0.2-alpha",
  "description": "Another lavalink wrapper but focus on stability and rich features",
  "scripts": {
    "build": "npx tsc --build --verbose",
    "prettier": "npx prettier -w ./src",
    "build:doc": "typedoc --readme ./md/Welcome.md --out docs/ --entryPointStrategy resolve src/.",
    "build:full": "npm run prettier && npm run build && npm run build:doc"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "lavalink",
    "wrapper",
    "lavalink-wrapper"
  ],
  "author": "RainyXeon",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.10",
    "dokdo": "^1.0.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mxssfd/typedoc-theme": "^1.1.3",
    "axios": "^1.6.7",
    "delay": "^6.0.0",
    "discord.js": "^14.12.1",
    "node-html-parser": "^6.1.12",
    "typedoc": "^0.25.8",
    "typedoc-material-theme": "^1.0.2",
    "undici": "^6.6.2",
    "ws": "^8.16.0"
  }
}
