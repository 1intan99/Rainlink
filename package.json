{
  "name": "rainlink",
  "version": "0.2.4-beta",
  "description": "Another lavalink wrapper but focus on stability and rich features",
  "repository": {
    "type": "git",
    "url": "https://github.com/RainyXeon/Rainlink.git"
  },
  "scripts": {
    "build": "npx tsc --build --verbose",
    "prettier": "npx prettier -w ./src",
    "build:doc": "typedoc --options ./typedoc.json --sort kind",
    "build:full": "npm run prettier && npm run build && npm run build:doc"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "bot",
    "music",
    "lavalink.js",
    "lavalink-api",
    "discord.js",
    "eris.js",
    "oceanic.js",
    "lavalink",
    "wrapper",
    "lavalink-wrapper",
    "rainlink"
  ],
  "author": "RainyXeon",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.10",
    "discord.js": "^14.12.1",
    "dokdo": "^1.0.1",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.4",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "node-html-parser": "^6.1.12",
    "simple-json-db": "^2.0.0",
    "ws": "^8.16.0"
  }
