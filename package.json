{
  "name": "rainlink",
  "version": "0.4.1-beta3",
  "description": "Another lavalink wrapper but focus on stability and rich features",
  "repository": {
    "type": "git",
    "url": "https://github.com/RainyXeon/Rainlink.git"
  },
  "scripts": {
    "build": "npx tsc --build --verbose",
    "prettier": "npx prettier -w ./src",
    "build:doc": "typedoc --options ./typedoc.json --sort kind",
    "build:full": "npm run prettier && npm run build:eslint && npm run build && npm run build:doc",
    "build:eslint": "npx eslint ./src --fix"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "bot",
    "music",
    "lavalink.js",
    "lavalink-api",
    "discord.js",
    "eris.js",
    "oceanic.js",
    "lavalink",
    "wrapper",
    "lavalink-wrapper",
    "rainlink"
  ],
  "author": "RainyXeon",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "discord.js": "^14.14.1",
    "dokdo": "^1.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.12",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.4.2",
    "undici-types": "^6.7.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "node-html-parser": "^6.1.12",
    "simple-json-db": "^2.0.0",
    "undici": "^6.7.1",
    "ws": "^8.16.0"
  }
}
